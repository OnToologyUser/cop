    <!-- https://raw.githubusercontent.com/Rel-incode/cop/master/cop.owl#Agent -->

    <Class rdf:about="https://raw.githubusercontent.com/Rel-incode/cop/master/cop.owl#Agent">
        <disjointWith rdf:resource="https://raw.githubusercontent.com/Rel-incode/cop/master/cop.owl#Fact"/>
        <disjointWith rdf:resource="https://raw.githubusercontent.com/Rel-incode/cop/master/cop.owl#Medium"/>
